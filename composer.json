{
  "name": "ems-project/elasticms",
  "description": "A preconfigured elasticms application",
  "authors": [{
    "name": "Mathieu De Keyzer",
    "email": "mathieu@dekeyzer.net"
  }
  ],
  "keywords": [
    "elasticms"
  ],
  "license": "MIT",
  "type": "project",
  "minimum-stability": "RC",
  "require": {
    "php": "^8.1",
    "ext-gd": "*",
    "ext-iconv": "*",
    "ext-json": "*",
    "ext-openssl": "*",
    "ext-tidy": "*",
    "doctrine/annotations": "^1.14",
    "doctrine/doctrine-bundle": "^2.8",
    "doctrine/doctrine-migrations-bundle": "^3.2",
    "doctrine/orm": "^2.6",
    "elasticms/core-bundle": "^5.0",
    "elasticms/client-helper-bundle": "^5.0",
    "elasticms/common-bundle": "^5.0",
    "elasticms/form-bundle": "^5.0",
    "elasticms/submission-bundle": "^5.0",
    "symfony/console": "^5.4",
    "symfony/dotenv": "^5.4",
    "symfony/flex": "^1.6",
    "symfony/form": "^5.4",
    "symfony/framework-bundle": "^5.4",
    "symfony/mailer": "^5.4",
    "symfony/monolog-bundle": "^3.8",
    "symfony/runtime": "^5.4",
    "symfony/security-bundle": "^5.4",
    "symfony/twig-bundle": "^5.4",
    "symfony/web-link": "^5.4"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "symfony/browser-kit": "^5.4",
    "symfony/css-selector": "^5.4",
    "symfony/debug-bundle": "^5.4",
    "symfony/phpunit-bridge": "^5.4",
    "symfony/web-profiler-bundle": "^5.4"
  },
  "config": {
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true,
    "allow-plugins": {
      "symfony/flex": true,
      "symfony/runtime": true
    }
  },
  "autoload": {
    "psr-4": {
      "App\\Admin\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Admin\\Tests\\": "tests/"
    }
  },
  "replace": {
    "symfony/polyfill-iconv": "*"
  },
  "scripts": {
    "test": "simple-phpunit",
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd"
    },
    "post-install-cmd": [
      "@auto-scripts"
    ],
    "post-update-cmd": [
      "@auto-scripts"
    ],
    "phpcs": "php-cs-fixer fix",
    "phpstan": "phpstan analyse",
    "phpunit": "phpunit"
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "extra": {
    "symfony": {
      "allow-contrib": false,
      "require": "^5.4"
    }
  }
}
